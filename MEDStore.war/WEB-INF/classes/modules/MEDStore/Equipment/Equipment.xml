<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.skyve.org/xml/document" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Equipment" xsi:schemaLocation="http://www.skyve.org/xml/document ../../../schemas/document.xsd">
    <persistent name="MED_Equipment"/>
    <singularAlias>Equipment</singularAlias>
    <pluralAlias>Equipment</pluralAlias>
    <iconStyleClass>fa fa-gear</iconStyleClass>
    <bizKey expression="Equipment - {name}"/>
    <attributes>
        <text required="true" name="name">
            <displayName>Name</displayName>
            <length>500</length>
        </text>
        <association required="true" type="aggregation" name="equipmentGroupID">
            <displayName>Equipment Group ID</displayName>
            <documentName>EquipmentGroup</documentName>
        </association>
    </attributes>
</document>