<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.skyve.org/xml/document" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Department" xsi:schemaLocation="http://www.skyve.org/xml/document ../../../schemas/document.xsd">
    <persistent name="MED_Department"/>
    <singularAlias>Department</singularAlias>
    <pluralAlias>Departments</pluralAlias>
    <iconStyleClass>fa fa-building</iconStyleClass>
    <bizKey expression="Department - {name}"/>
    <attributes>
        <text required="true" name="name">
            <displayName>Name</displayName>
            <length>500</length>
        </text>
    </attributes>
</document>